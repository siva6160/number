package factorial;
import java.util.*;
public class basics {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int num;
		int fact=1;
		num=sc.nextInt();
		
		for(int i=1;i<=num;i++){
			//System.out.println(i);
			fact=fact*i;
		}
			System.out.println("the factorial of given number "+fact);
		}

	}


