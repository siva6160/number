package array;
//import java.util.*;
public class basicarray {

	public static void main(String[] args) {
		int arr[]=new int[5];//creating an array
		arr[0]=9;
		arr[1]=0;
		arr[2]=16;
		arr[3]=55;//declereation
		arr[4]=9;
		//arr[5]=86;
		for(int i=0;i<arr.length;i++){
			System.out.print(" "+arr[i]);//instatilition
		}
	}

}
