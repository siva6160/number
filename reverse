package reverse;
import java.util.*;
public class reversinng {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num;
		int sum=0;
		int r;
		Scanner sc=new Scanner(System.in);
		num=sc.nextInt();
		while (num!=0){
			r=num%10;
			sum=sum*10+r;
			num=num/10;
			//System.out.println(r);
		}
			
		System.out.print(sum);	

	}

}
